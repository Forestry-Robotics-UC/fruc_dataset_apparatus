<launch>
    <arg name="output"                    default="screen"/>
    <arg name="fisheye_width"             default="640"/>
    <arg name="fisheye_height"            default="480"/>
    <arg name="enable_fisheye"            default="false"/>

  <arg name="depth_width" default="640" />
  <arg name="depth_height" default="480" />
  <arg name="enable_depth" default="true" />
  <arg name="infra_width"               default="640"/>
  <arg name="infra_height"              default="480"/>
  <arg name="enable_infra"              default="true"/>
  <arg name="infra_rgb"                 default="false"/>

  <arg name="color_width"               default="640"/>
  <arg name="color_height"              default="480"/>
  <arg name="enable_color"              default="true"/>


  <arg name="fisheye_fps" default="30" />
  <arg name="depth_fps" default="30" />
  <arg name="infra_fps" default="30" />
  <arg name="color_fps" default="30" />
  <arg name="gyro_fps" default="400" />
  <arg name="accel_fps" default="250" />
  <arg name="enable_gyro"               default="true"/>
  <arg name="enable_accel"              default="true"/>
  <arg name="unite_imu_method"          default="linear_interpolation"/>

  <arg name="enable_confidence"         default="false"/>

  <arg name="enable_pointcloud"         default="true"/>
  <arg name="enable_sync"               default="false"/>
  <arg name="align_depth"               default="true"/> 
  <arg name="filters"                   default=""/>rs
  <arg name="tf_publish_rate"           default="0"/>
  <arg name="clip_distance"             default="-2"/>
  <arg name="initial_reset"             default="true"/>

  <include file="$(find realsense2_camera)/launch/rs_camera.launch">
    <arg name="output"                          default="$(arg output)"/>
    <arg name="fisheye_width"                   default="$(arg fisheye_width)"/>
    <arg name="fisheye_height"                  default="$(arg fisheye_height)"/>
    <arg name="enable_fisheye"                  default="$(arg enable_fisheye)"/>
    <arg name="depth_width"                     default="$(arg depth_width)"/>
    <arg name="depth_height"                    default="$(arg depth_height)"/>
    <arg name="enable_depth"                    default="$(arg enable_depth)"/>
    <arg name="infra_width"                     default="$(arg infra_width)"/>
    <arg name="infra_height"                    default="$(arg infra_height)"/>
    <arg name="enable_infra"                    default="$(arg enable_infra)"/>
    <arg name="infra_rgb"                       default="$(arg infra_rgb)"/>
    <arg name="color_width"                     default="$(arg color_width)"/>
    <arg name="color_height"                    default="$(arg color_height)"/>
    <arg name="enable_color"                    default="$(arg enable_color)"/>
    <arg name="fisheye_fps"                     default="$(arg fisheye_fps)"/>
    <arg name="depth_fps"                       default="$(arg depth_fps)"/>
    <arg name="infra_fps"                       default="$(arg infra_fps)"/>
    <arg name="color_fps"                       default="$(arg color_fps)"/>
    <arg name="gyro_fps"                        default="$(arg gyro_fps)"/>
    <arg name="accel_fps"                       default="$(arg accel_fps)"/>
    <arg name="enable_gyro"                     default="$(arg enable_gyro)"/>
    <arg name="enable_accel"                    default="$(arg enable_accel)"/>
    <arg name="unite_imu_method"                default="$(arg unite_imu_method)"/>
    <arg name="enable_pointcloud"               default="$(arg enable_pointcloud)"/>
    <arg name="enable_sync"                     default="$(arg enable_sync)"/>
    <arg name="align_depth"                     default="$(arg align_depth)"/> 
    <arg name="filters"                         default="$(arg filters)"/>
    <arg name="tf_publish_rate"                 default="$(arg tf_publish_rate)"/>
    <arg name="initial_reset"                   default="$(arg initial_reset)"/>
    <arg name="clip_distance"                   default="$(arg clip_distance)"/>
    <arg name="enable_confidence"               default="$(arg enable_confidence)"/>
  </include>
</launch>
