<launch>

	<arg name="model" default="$(find sensor-tools)/urdf/mynteye_S3010.urdf"/>

	<!--  Remap topics from XSENS IMU Driver -->
	<remap from="velocity" to="imu/velocity"/>
	<remap from="analog_in1" to="imu/analog_in1"/>
	<remap from="analog_in2" to="imu/analog_in2"/>
	<remap from="diagnostics" to="imu/diagnostics"/>
	<remap from="diagnostics" to="imu/diagnostics"/>
	<remap from="temperature" to="imu/temperature"/>
	<remap from="imu_data_str" to="imu/imu_data_str"/>

	<!--<include file="$(find mynt_eye_ros_wrapper)/launch/laserscan/mynteye_s1030_laserscan.launch" >
		<arg name="mesh_file" value="/rviz/mesh/S1030-0315.obj" />
	</include>-->

	<include file="$(find mynt_eye_ros_wrapper)/launch/mynteye.launch" />

	<include file="$(find xsens_driver)/launch/xsens_driver.launch" >
		<arg name="frame_id" value="xsens_frame" />
	</include>

	<!--<param name="robot_description" textfile="$(arg model)" />
	<node pkg="robot_state_publisher" type="robot_state_publisher" name="robot_state_publisher" output="screen">
            <remap from="joint_state" to="joint_state"/>
            <param name="publish_frequency" type="double" value="15.0"/>
    </node>-->

</launch>
